//Lab4
Map.setCenter(-156.47, 20.89, 9);

var image8 = ee.Image(Lsat  
  .filterDate('2021-01-01', '2021-12-31')
  .filterBounds(Maui)
  .sort('CLOUD_COVER')
  .first()
  );
  
var trueColorL8 = {
        bands: ["SR_B4", "SR_B3", "SR_B2"],
        min: 0,
        max: 30000
        };

Map.addLayer(image8, trueColorL8, 'Maui TC');

var classNames = MauiUrban.merge(MauiWater).merge(VegMaui).merge(DescMaui);
var bands = ['SR_B2', 'SR_B3', 'SR_B4', 'SR_B5', 'SR_B6', 'SR_B7'];
var training = image8.select(bands).sampleRegions({
  collection: classNames,
  properties: ['landcover'],
  scale: 30
});

var classifier = ee.Classifier.cart().train({
  features: training,
  classProperty: 'landcover',
  inputProperties: bands
});
var classified = image8.select(bands).classify(classifier);
Map.centerObject(classNames, 11);
Map.addLayer(classified,
{min: 0, max: 3, palette: ['pink', 'blue', 'green','yellow']},
'classification');