        .filterDate("2019-06-01", "2019-06-10")
        .filterBounds(Cairo)
        .sort("CLOUD_COVERAGE_ASSESSMENT")
        .first()
  );
  
var image4 = ee.Image(sent2 
        .filterDate("2019-12-01", "2019-12-10")
        .filterBounds(Cairo)
        .sort("CLOUD_COVERAGE_ASSESSMENT")
        .first()
  );

